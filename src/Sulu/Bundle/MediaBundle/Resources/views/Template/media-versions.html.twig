<form id="media-versions">
    <div class="grid">
        <div class="grid-row">
            <div class="media-edit-versions-links">
                <% _.each(media.versions, function(versionData, versionNumber) { %>
                    <a class="media-edit-versions-link <% if (versionNumber == media.version) { %>active<% } %>" href="<%= versionData.url %>" target="_blank">
                        <%= translate('sulu.media.version') %> <%= versionNumber %>
                        <% if (versionNumber == media.version) { %>
                            (<%= translate('sulu.media.version.active') %>)
                        <% } %>
                        <span class="media-edit-versions-created"><%= app.sandbox.date.format(versionData.created) %></span>
                    </a>
                <% }); %>
            </div>
        </div>
    </div>
</form>
