<?xml version="1.0" ?>
<massive-search-mapping xmlns="http://massive.io/schema/dic/massive-search-mapping">

    <mapping class="Sulu\Bundle\ContactBundle\Entity\Account">
        <id name="id" />
        <title property="name" />

        <fields>
            <field name="name" type="string" />
        </fields>

        <context name="admin">
            <url expr="'/admin/#contacts/accounts/edit:' ~ object.getId() ~ '/details'" />
            <index name="admin_account" />
        </context>
    </mapping>

</massive-search-mapping>
