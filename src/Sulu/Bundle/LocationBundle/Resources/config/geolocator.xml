<?xml version="1.0" encoding="utf-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="sulu.location.guzzle_client.class">Sulu\Bundle\LocationBundle\Content\Types\LocationContent</parameter>
        <parameter key="sulu_location.geolocator.nominatim.class">Sulu\Bundle\LocationBundle\Geolocator\Service\NominatimGeolocator</parameter>
    </parameters>

    <services>
        <!-- Map services -->
        <service id="sulu_location.guzzle_client" class="%sulu_location.guzzle_client.class%" />

        <!-- Geolocator services -->
        <service id="sulu_location.gelocator.nominatim" class="%sulu_location.geolocator.nominati.class%">
            <tag name="sulu_location.geolocator" alias="geolocator" />
        </service>
    </services>
</container>


