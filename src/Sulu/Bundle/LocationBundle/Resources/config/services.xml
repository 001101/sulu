<?xml version="1.0" encoding="utf-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="sulu.content.type.location.class">Sulu\Bundle\LocationBundle\Content\Types\LocationContent</parameter>
    </parameters>

    <services>
        <!-- content-types -->
        <service id="sulu.content.type.location" class="%sulu.content.type.location.class%">
            <argument type="service" id="sulu_content.node_repository"/>
            <argument>%sulu.content.type.location.template%</argument>
        </service>

    </services>
</container>

